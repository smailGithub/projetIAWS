    <wsdl:operation name="InscriptionOperation">
      <soap:operation soapAction="http://IAWS/Inscription"/>
      <wsdl:input name="InscriptionRequest">
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  
  <wsdl:binding name="getVoisinBinding" type="tns:getVoisin">
